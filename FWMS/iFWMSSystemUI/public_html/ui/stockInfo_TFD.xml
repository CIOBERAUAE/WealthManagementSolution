<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="stockInfo_TFD">
    <default-activity>StockInfo</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="StockInfo">
      <description>StockInformation</description>
      <display-name>StockInformation</display-name>
      <page>/ui/stock/jsff/stockInfoView.jsff</page>
    </view>
    <view id="StockInfoEdit">
      <description>StockInformationEdit</description>
      <display-name>StockInformation</display-name>
      <page>/ui/stock/jsff/stockInfoEdit.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>StockInfo</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>edit</from-outcome>
        <to-activity-id>StockInfoEdit</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
